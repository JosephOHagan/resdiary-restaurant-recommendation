    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa"
        crossorigin="anonymous"></script>
</head>

<body>
    <!--Header-->
    <div class="res-navbar-wrapper">
        <nav class="navbar navbar-default res-navbar">
            <div class="container-fluid">
                <div class="navbar-header res-center-xs">
                    <a class="navbar-brand" href="#">
                        <img class="res-logo" alt="Resdiary Recommendations" src="../img/res_logo.png">
                    </a>
                </div>
                <h2 class="navbar-right res-nav-title res-hidden-xs">Recommendations Demo</p>
            </div>
            <div class="res-navbar-header-extra"></div>
        </nav>
    </div>
    <!--//Header-->
    <!--Main Content-->
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12 res-title">
                <h2><strong>PICK A USER</strong></h2>
                <div class="res-line-decor"></div>
            </div>
        </div>
        <div class="row">
            <!--User 1-->
            <div class="col-sm-3 col-xs-12" id="user1">
                <a href="/new_demo/user/<%= ids[0] %>">
                    <div class="res-recommendation-user">
                        <img src="../img/<%= genders[0] %>" class="img-responsive" />
                        <h4><%= ids[0] %></h4>
                    </div>
                    
                </a>
            </div>
            <!--//Col: User 1-->
            <!--User 2-->
            <div class="col-sm-3 col-xs-12" id="user2">
                <a href="/new_demo/user/<%= ids[1] %>">
                    <div class="res-recommendation-user">
                        <img src="../img/<%= genders[1]%>" class="img-responsive" />
                        <h4><%= ids[1] %></h4>
                    </div>
                    
                </a>
            </div>
            <!--//Col: User 2-->
            <!--User 3-->
            <div class="col-sm-3 col-xs-12" id="user3">
                <a href="/new_demo/user/<%= ids[2] %>">
                    <div class="res-recommendation-user">
                        <img src="../img/<%= genders[2]%>" class="img-responsive" />
                        <h4><%= ids[2] %></h4>
                    </div>
                    
                </a>
            </div>
            <!--//Col: User 3-->
            <!--User 4-->
            <div class="col-sm-3 col-xs-12" id="user4">
                <a href="/new_demo/user/<%= ids[3] %>">
                    <div class="res-recommendation-user">
                        <img src="../img/<%= genders[3]%>" class="img-responsive" />
                        <h4><%= ids[3] %></h4>
                    </div>
                    
                </a>
            </div>
            <!--//Col: User 4-->
        </div>
    </div>
    <!--//Main Content-->
</body>
